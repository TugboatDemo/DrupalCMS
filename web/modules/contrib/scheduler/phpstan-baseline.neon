# ------------------------------------------------------------------------------
# This phpstan-baseline.neon file contains the messages for code that will
# not be changed. This is a permanent list of ignored messages. The order and
# format of the messages matches the baseline file generated by the pipeline job
# to make file comparisions possible.
# ------------------------------------------------------------------------------
parameters:
  ignoreErrors:

    #---------------------------------------------------------------------------------------------------------------------------
    # scheduler_form_pane was added for ctools in 7.x, probably does not work in 8.x+
    #---------------------------------------------------------------------------------------------------------------------------
    -
      message: '#^Instantiated class ctools_context_required not found\.$#'
      identifier: class.notFound
      count: 1
      path: plugins/content_types/scheduler_form_pane.inc

    #---------------------------------------------------------------------------------------------------------------------------
    # Mapping callback for the Feeds module is still not ported to Drupal 8.
    #---------------------------------------------------------------------------------------------------------------------------
    -
      message: '#^Instantiated class FeedsValidationException not found\.$#'
      identifier: class.notFound
      count: 1
      path: scheduler.module

    #---------------------------------------------------------------------------------------------------------------------------
    # scheduler_migration_plugins_alter() will likely be rewritten or removed before compatibility with Drupal 12.
    # The class is deprecated in drupal:11.3.0 and is removed from drupal:12.0.0. There is no  replacement.
    # This currently needs reportUnmatched: false because the warning is not produced in Drupal 11.2
    #---------------------------------------------------------------------------------------------------------------------------
    -
      message: '#^Call to method getSystemData\(\) of deprecated class Drupal\\migrate_drupal\\Plugin\\migrate\\source\\DrupalSqlBase#'
      identifier: method.deprecatedClass
      count: 1
      path: scheduler.module
      reportUnmatched: false

    #---------------------------------------------------------------------------------------------------------------------------
    # rules_defaults.inc is still 7.x, not converted to 8.x+ and may never be, as development on the Rules project has slowed.
    #---------------------------------------------------------------------------------------------------------------------------
    -
      message: '#^Function rule not found\.$#'
      identifier: function.notFound
      count: 4
      path: scheduler_rules_integration/scheduler_rules_integration.rules_defaults.inc

    -
      message: '#^Function rules_condition not found\.$#'
      identifier: function.notFound
      count: 2
      path: scheduler_rules_integration/scheduler_rules_integration.rules_defaults.inc

    -
      message: '#^Function rules_reaction_rule not found\.$#'
      identifier: function.notFound
      count: 2
      path: scheduler_rules_integration/scheduler_rules_integration.rules_defaults.inc

    #---------------------------------------------------------------------------------------------------------------------------
    # Allow $p1-$p8 and $message in throwSchedulerException()
    #---------------------------------------------------------------------------------------------------------------------------
    -
      message: '#^Variable \$(p\d|message) might not be defined\.$#'
      identifier: variable.undefined
      count: 9
      path: src/SchedulerManager.php
